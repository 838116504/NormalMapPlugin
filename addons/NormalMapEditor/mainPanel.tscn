code = "shader_type canvas_item;

uniform bool normal_preview = false;
uniform sampler2D normal_texture;

void fragment(){
	COLOR = texture(TEXTURE,UV);
	NORMAL = texture(normal_texture, UV).xyz*2.0-1.0;
	//NORMAL = normalize(vec3(0.2, 0.2, 0.8));
	if (normal_preview){
		if (COLOR.a > 0.1)
		{
			vec4 normalColor = texture(normal_texture, UV);
			if (normalColor.a > 0.0)
				COLOR = normalColor;
		}
		else
			discard;
	}
}


void light(){
	if (normal_preview){
		LIGHT = vec4(0.0)
	}
}"

